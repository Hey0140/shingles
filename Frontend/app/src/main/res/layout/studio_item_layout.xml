<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="280dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/studio_item_imageview"
        android:layout_width="match_parent"
        android:layout_height="169dp"
        android:src="@drawable/image_studio"
        android:scaleType="centerCrop"
        />


    <ImageView
        android:id="@+id/studio_good_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="@id/studio_item_imageview"
        android:src="@drawable/good_icon"
        />
    <TextView
        android:id="@+id/studio_item_good_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        app:layout_constraintLeft_toRightOf="@id/studio_good_icon"
        app:layout_constraintBottom_toBottomOf="@id/studio_item_imageview"
        android:text="87% 리얼후기 (1,980개)"
        android:fontFamily="@font/gmarket"
        android:textColor="@color/white"
        />


    <TextView
        android:id="@+id/studio_item_tile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/studio_item_imageview"
        app:layout_constraintLeft_toLeftOf="@id/studio_item_imageview"
        android:text="비쥬 합주실 1호점"
        android:textSize="22dp"
        android:fontFamily="@font/gmarket"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="9dp"
        android:textColor="@color/black"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/studio_item_constraint1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/studio_item_tile"
        android:layout_marginTop="11dp"
        app:layout_constraintLeft_toLeftOf="@id/studio_item_imageview"
        android:layout_marginLeft="20dp"
        >
        <TextView
            android:id="@+id/studio_item_hour"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1시간"
            android:fontFamily="@font/gmarket"
            android:textSize="15dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_discount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13%"
            android:fontFamily="@font/gmarket"
            android:textSize="12dp"
            android:textColor="#FF4747"
            app:layout_constraintLeft_toRightOf="@id/studio_item_hour"
            android:layout_marginLeft="11dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_account"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="20,000원"
            android:fontFamily="@font/gmarket"
            android:textSize="22dp"
            app:layout_constraintLeft_toRightOf="@id/studio_item_discount"
            android:layout_marginLeft="5dp"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_origin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="23,000원"
            android:fontFamily="@font/gmarket"
            android:textSize="12dp"
            app:layout_constraintLeft_toRightOf="@id/studio_item_account"
            android:layout_marginLeft="5dp"
            android:textColor="@color/grey"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/studio_item_constraint2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/studio_item_constraint1"
        android:layout_marginTop="3dp"
        app:layout_constraintLeft_toLeftOf="@id/studio_item_imageview"
        android:layout_marginLeft="20dp"
        >
        <TextView
            android:id="@+id/studio_item_hour2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2시간"
            android:fontFamily="@font/gmarket"
            android:textSize="15dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_discount2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13%"
            android:fontFamily="@font/gmarket"
            android:textSize="12dp"
            android:textColor="#FF4747"
            app:layout_constraintLeft_toRightOf="@id/studio_item_hour2"
            android:layout_marginLeft="9dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_account2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="40,000원"
            android:fontFamily="@font/gmarket"
            android:textSize="22dp"
            app:layout_constraintLeft_toRightOf="@id/studio_item_discount2"
            android:layout_marginLeft="5dp"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/studio_item_origin2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="46,000원"
            android:fontFamily="@font/gmarket"
            android:textSize="12dp"
            app:layout_constraintLeft_toRightOf="@id/studio_item_account2"
            android:layout_marginLeft="5dp"
            android:textColor="@color/grey"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>